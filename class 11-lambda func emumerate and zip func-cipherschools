#lambda function
lambda a,b:a+b

names=["Knox","Alex","Carter","Jason","Aiden"]
for i, name in enumerate(names):
    print(i,"-",name)

#packing and unpacking values
a=7
b=9
a,b=b,a
print(a,b)


names=["Knox","Alex","Carter","Jason","Aiden"]
scores=[50, 80, 90, 70]
for i, name in enumerate(names):
    score = scores[i]
    print(name,"-",score)


names=["Knox","Alex","Carter","Jason","Aiden"]
scores=[50, 80, 90, 70]
for a in zip(names,scores):
    print(a)
