def show_loading():
    for i in range(3):
        print(i)
        print("loading" , "."*(i +1))

a=5
b=7

print(a+b)
show_loading()

print(a-b)
show_loading()

print(a*b)
show_loading()

def sheldon_knock(name):
    for i in range(3):
        print("knock knock knock penny",name)

sheldon_knock

def add(a,b):
    return a+b

a=add(34,90)
print(a)
