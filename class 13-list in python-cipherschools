print(len("Dante"))

print(len[1,5,9,7,6])

print([1]*5)

a=[1,4,5,3,2,4]
for x in a:
    print(x)

a=[1,2,4,6]
a.append(5)
print(a)

a=[1,2,4,6]
a.pop(2)
print(a)

a=[1,2,4,6,2]
a.remove(2)
print(a)


a=[1,2,4,6,3,5]
a.sort()
print(a)


a=[1,2,4,6,3,5]
list(reversed(a))
print(a)

a=[1,2,4,6,3,5]
x=list(map(lambda x:x**2,a))
print(x)
